{
  "name": "@laserfiche/lf-repository-api-client",
  "type": "module",
  "version": "1.1.2",
  "description": "Laserfiche Repository API Client library for TypeScript.",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/src/index.js",
    "dist/src/index.d.ts",
    "README.md"
  ],
  "scripts": {
    "test": "npm run test:node",
    "test:node": "node --experimental-vm-modules --experimental-fetch node_modules/jest/bin/jest.js --rootDir=test --config jest.node.config.js --runInBand",
    "test:browser": "node --experimental-vm-modules --experimental-fetch node_modules/jest/bin/jest.js --rootDir=test --config jest.jsdom.config.js --runInBand",
    "test:all": "node --experimental-vm-modules --experimental-fetch node_modules/jest/bin/jest.js --rootDir=test --config jest.node.config.js --runInBand && node --experimental-vm-modules node_modules/jest/bin/jest.js --rootDir=test --config jest.jsdom.config.js --runInBand",
    "build": "nswag run && npm run clean && tsc -b",
    "build:watch": "nswag run && tsc -b --watch",
    "clean": "tsc -b --clean",
    "nswag": "nswag run"
  },
  "keywords": [
    "Laserfiche"
  ],
  "author": "Laserfiche",
  "url": "https://www.laserfiche.com/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Laserfiche/lf-repository-api-client-js"
  },
  "devDependencies": {
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jest": "^27.4.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.12",
    "babel-jest": "^27.5.1",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^27.5.1",
    "jest-junit": "^13.0.0",
    "nswag": "^13.15.5",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@laserfiche/lf-api-client-core": "^1.1.4",
    "@laserfiche/lf-js-utils": "^4.0.5"
  }
}
